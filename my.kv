<my_grid>
    nocal: nocal
    socal: socal
    desert: desert
    vancouver: vancouver
    seattle: seattle
    other: other
    start: start
    choicelabel: choicelabel
    scorelabel: scorelabel
    message: message


    GridLayout:
        cols:1
        size: root.width -200, root.height -200
        pos: 100, 100
        Label:
            id:message
            text: ""
            
        Label:
            id:scorelabel
            text: ""

        Label:
            id:choicelabel
            text: ""

        GridLayout:
            cols: 7

            Button:
                id: nocal
                text: "nocal"
                on_press:root.NOCALCHECK()
            Button:
                id: socal
                text: "socal"
                on_press:root.SOCALCHECK()
            Button:
                id: desert
                text: "desert"
                on_press:root.DESERTCHECK()
            Button:
                id: start
                text: "start"
                on_press:root.btn()
            Button:
                id: vancouver
                text: "vancouver"
                on_press:root.VANCOUVERCHECK()
            Button:
                id: seattle
                text: "seattle"
                on_press:root.SEATTLECHECK()
            Button:
                id: other
                text: "other"
                on_press:root.OTHERCHECK()

